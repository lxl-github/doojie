			<!-- quartz -->
			<dependency>
	            <groupId>org.quartz-scheduler</groupId>
	            <artifactId>quartz</artifactId>
	            <version>2.2.1</version>
        	</dependency>
			<!-- end quartz -->
			
			<!-- jdom xml -->
			<dependency>
				<groupId>jdom</groupId>
				<artifactId>jdom</artifactId>
				<version>1.0</version>
			</dependency>
